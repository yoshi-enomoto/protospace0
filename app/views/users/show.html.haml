.container.user-page
  %header.user-nav
    .media
      .meida-left
        %a{href: "#"}
          = image_tag current_user.avatar.url, style: "width: 64px; height: 64px;"
          -# 下記の記述でも取り出せる。
          -# = image_tag "#{current_user.avatar}", style: "width: 64px; height: 64px;"
          / %img.media-object{alt: "64x64", "data-holder-rendered": "true", "data-src": "holder.js/64x64", src: "data:image/svgxml;base64,", style: "width: 64px; height: 64px;"}
      .meida-body
        %h4#top-aligned-media.media-heading
          Top aligned media
          %span.label.labal-default.project-number
            29
            %i Projects
        .proto-user
          %span.degree #{current_user.position}
        .row
          .col-md-6
            %h4 Profile
            %p #{current_user.profile}
          .col-md-6
            %h4 Works
            %p #{current_user.occupation}
